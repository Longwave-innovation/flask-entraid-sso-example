SECRET_KEY=your-secret-key-here

# Auth Provider: 'entraid' or 'cognito'
AUTH_PROVIDER=entraid

# Entra ID Configuration (when AUTH_PROVIDER=entraid)
TENANT_ID=your-tenant-id
CLIENT_ID=your-entraid-client-id
CLIENT_SECRET=your-entraid-client-secret

# AWS Cognito Configuration (when AUTH_PROVIDER=cognito)
COGNITO_DOMAIN=https://your-domain.auth.region.amazoncognito.com
CLIENT_ID=your-cognito-client-id
CLIENT_SECRET=your-cognito-client-secret
COGNITO_REGION=eu-south-1

# App Configuration
HOST=http://localhost
PORT=8080
CALLBACK_PATH=/login/callback
LOGIN_PATH=/login